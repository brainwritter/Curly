{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "App": [
        "App/**/*"
      ],
      "Setup/*": [
        "App/Setup/*"
      ],
      "Domains/*": [
        "App/Domains/*"
      ],
      "curly-ui": [
        "UIkit/src/index"
      ]
    },
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js"
  ]
}